original":{"line":66,"column":27},"generated":{"line":1,"column":2093}},{"source":"views/PendingAuthorisationView.js","name":"PropTypes","original":{"line":67,"column":15},"generated":{"line":1,"column":2098}},{"source":"views/PendingAuthorisationView.js","name":"PropTypes","original":{"line":67,"column":15},"generated":{"line":1,"column":2110}},{"source":"views/PendingAuthorisationView.js","name":"func","original":{"line":67,"column":25},"generated":{"line":1,"column":2112}},{"source":"views/PendingAuthorisationView.js","name":"func","original":{"line":67,"column":25},"generated":{"line":1,"column":2120}},{"source":"views/PendingAuthorisationView.js","name":"PendingAuthorisationView","original":{"line":70,"column":15},"generated":{"line":1,"column":2126}},{"source":"views/PendingAuthorisationView.js","name":"PendingAuthorisationView","original":{"line":70,"column":15},"generated":{"line":1,"column":2134}},{"source":"views/PendingAuthorisationView.js","name":"PendingAuthorisationView","original":{"line":70,"column":15},"generated":{"line":1,"column":2142}}],"sources":{"views/PendingAuthorisationView.js":"import React, {Component} from 'react';\nimport PropTypes from 'prop-types';\nimport {filterIP} from '../utils';\nimport LoaderView from './common/LoaderView';\n\nclass PendingAuthorisationView extends Component {\n  constructor(props) {\n    super(props);\n    this.state = {\n      isLoading: false\n    };\n  }\n\n  onAcceptRequestClick() {\n    const {key} = this.props.authorisation;\n    const {onAcceptClick} = this.props;\n    \n    this.setState({isLoading: true});\n    onAcceptClick(key);\n  }\n  \n  render() {\n    const {isLoading} = this.state;\n    const {authorisation, onDenyClick} = this.props;\n    \n    return (\n      <div>\n        <br />\n        <p\n          className=\"pending-authorizations-text\"\n          key={authorisation.key}\n        >\n          Someone requested to log into this app from a{' '}\n          <span className=\"bold\">{authorisation.deviceInfo.browser}</span>{' '}\n          browser in{' '}\n          <span className=\"bold\">{authorisation.deviceInfo.name}</span>,\n          from the IP{' '}\n          <span className=\"bold\">\n            {filterIP(authorisation.deviceInfo.ipAddress)} (\n            {authorisation.deviceInfo.city})\n          </span>{' '}\n          at <span className=\"bold\">{authorisation.deviceInfo.time}</span>\n        </p>\n        {isLoading && <LoaderView className=\"pending-authorizations-loader\" />}\n        <button\n          className=\"btn-alt fullwidth pending-authorizations-btn\"\n          onClick={() => this.onAcceptRequestClick()}\n          disabled={isLoading}\n        >\n          ACCEPT REQUEST <br />\n          <span className=\"click-cost\">costs 1 click</span>\n        </button>\n        <button\n          className=\"btn fullwidth\"\n          onClick={() => onDenyClick(authorisation.key)}\n        >\n          Deny request\n        </button>{' '}\n      </div>\n    );\n  }\n}\n\nPendingAuthorisationView.propTypes = {\n  authorisation: PropTypes.object,\n  onAcceptClick: PropTypes.func,\n  onDenyClick: PropTypes.func\n};\n\nexport default PendingAuthorisationView;\n"},"lineCount":null}},"hash":"bed4c6ade984ff8e74e7ce91055a813d","cacheData":{"env":{}}}