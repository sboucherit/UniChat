  "main": "index.js",
  "license": "MIT",
  "dependencies": {
    "components": "^0.1.0",
    "date-fns": "^1.29.0",
    "fbemitter": "^2.1.1",
    "localforage": "^1.7.3",
    "moment": "^2.22.2",
