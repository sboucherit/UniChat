    "jsdom": "^13.1.0",
    "jsdom-global": "^3.0.2",
    "mocha": "^5.2.0",
    "node-sass": "^4.9.0",
    "openzeppelin-solidity": "^2.1.1",
    "parcel-bundler": "^1.9.4",
    "prettier-eslint": "^8.8.2",
